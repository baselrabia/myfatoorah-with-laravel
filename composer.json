{
    "name": "baselrabia/myfatoorah-with-laravel",
    "description": "integration of myfatoorah with laravel framework for php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "baselrabia",
            "email": "baselrabia@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Basel\\MyFatoorah\\": "src/",
            "Basel\\MyFatoorah\\Tests\\": "tests/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "~6.5.2|~7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Basel\\MyFatoorah\\MyFatoorahServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.11"
    }
}
